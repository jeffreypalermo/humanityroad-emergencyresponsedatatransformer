@model List<AgencyModel>
@using HRERDataTransformer.Models
@{
    ViewBag.Title = "Home Page";
    
}


@foreach (var agency in Model)
{
    <text> [[Main Page]] | [[Emergency agencies using social media]] | [[@agency.State emergency agencies using social media]] Social media contacts sourced from '''Humanity Road''' [http://www.humanityroad.org/]
    <br/>
        ==@agency.AgencyType==<br/>
@if (@agency.Twitter1 != null)
{
    <text>*@agency.Twitter1*<br/></text>
}
@if (@agency.Twitter2 != null)
{
    <text>*@agency.Twitter2*<br/></text>
}
@if (@agency.Twitter3 != null)
{
    <text>*@agency.Twitter3*<br/></text>
}

</text>
}